!!! Strict
%html
	%head
		%link{:rel => "stylesheet", :href => "css/normalize.css"}/
		%link{:rel => "stylesheet", :href => "css/style.css"}/
	%body
		#full_container
			#loading
				#loading_container
					Cargando
			#stage 
				#header
					#logo
						logo
					#list
						- (1..4).each do |i|
							%a{id:"list_item#{i}",href:"#"}
								Item #{i}
				#menu_container
					#menu
						%a.selected{id:"menu_item1",href:"#"}
							Home
						- (2..4).each do |i|
							%a{id:"menu_item#{i}",href:"#"}
								Item #{i}
						%a{id:"menu_item5",href:"#"}
							Gallery
				#home 
					#home_container
						%h1
							Lorem ipsum dolor sit amet, consectetur adipisicing elit. Quibusdam molestias dolorum natus vel quidem doloremque. Laboriosam sit nulla modi sint, eum ad voluptatum ex, minus culpa. Sunt quisquam, odio adipisci.
				#bg_image					
					.intro_img.frame_1#intro_img1
				#intro
					- (1..3).each do |i|
						.intro_section{id:"intro_section#{i}"}
							
							.highlight_container
								%h1
									Highlight_#{i}
								%p 
									Lorem ipsum dolor sit amet, consectetur adipisicing elit. Nemo nobis aperiam hic? Numquam optio quaerat, eligendi commodi, ab molestias animi magni omnis distinctio vero natus hic enim sit, ut libero.
				#gallery
					#cont_gallery
						- (1..3).each do |i|
							%a.item_gallery{id:"item_gallery_#{i}",href:"#"}
								.gallery_item_container
									
				#gallery_popup
					#popup_container
						%a#popup_close{href:"#"}
							cerrar
						#popup_slider
							- (1..8).each do |i|
								.popup_item{id:"popup_item_#{i}"}
									.popup_item_container
										pop up item #{i}

						
						#popup_left_container
							#popup_left
						#popup_right_container
							#popup_right
						#popup_buttons_container
							- (1..8).each do |i|								
								%a.popup_button{id:"popup_button_#{i}",href:"#",page:"#{i-1}"}
									item #{i}	


	
		%script{type:"text/javascript",src:"js/libs/easing.js"}
		%script{type:"text/javascript",src:"js/script.js"}
		%script{type:"text/javascript",src:"js/fmw/core.js"}
		%script{type:"text/javascript",src:"js/fmw/preload_img.js"}
		%script{type:"text/javascript",src:"js/fmw/animation.js"}
		%script{type:"text/javascript",src:"js/fmw/simple_gallery.js"}
		%script{type:"text/javascript",src:"js/fmw/spritefy.js"}